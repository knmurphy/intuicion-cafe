---
layout: layouts/base.njk
section: post
---

<article>
  <h1>{{ title }}</h1>
  <p>
    <small>
      <time datetime="{{ page.date | machineDate }}">{{ page.date | readableDate }}</time> by <a href="/authors/{{ author | slug }}/">{{ author }}</a>
    </small>
  </p>


<iframe
              src="https://roast.world/profiles/VTJGc2RHVmtYMTltQXJEbHZLWW43aFhVSjZDVXpEODYwbUpXL1B0RERLM3o5RVhPdFE0UWdManA0VUYyZllmag?size=default"
              title="puebla natural tlacuilotepec #4"
              frameborder="0"
              sandbox="allow-scripts allow-same-origin"
              allowfullscreen
              width="640"
              height="480"
              ></iframe>

  {{ layoutContent | safe }}

  {% if tags %}
    <p>
    {% for tag in tags %}
      {%- if tag != "post" -%}
        {% set tagUrl %}/tags/{{ tag }}/{% endset %}
        <a href="{{ tagUrl | url }}" rel="tag">{{ tag }}</a>
      {%- endif -%}
    {% endfor %}
    </p>
  {% endif %}

</article>

<nav>
  <a href="{{ '/blog/' | url }}">‚Üê Blog index</a>
</nav>
